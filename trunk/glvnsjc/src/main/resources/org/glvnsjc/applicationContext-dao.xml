<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
  xsi:schemaLocation="
          http://www.springframework.org/schema/beans 
          http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		  http://www.springframework.org/schema/aop 
          http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	      http://www.springframework.org/schema/tx 
          http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">


  <bean id="schoolDao" class="org.glvnsjc.internal.dao.jpa.GenericNameDaoJpa">
    <constructor-arg value = "org.glvnsjc.model.domain.School" />
  </bean>

  <bean id="branchDao" class="org.glvnsjc.internal.dao.jpa.GenericNameDaoJpa">
    <constructor-arg value = "org.glvnsjc.model.domain.Branch" />
  </bean>

  <bean id="courseDao" class="org.glvnsjc.internal.dao.jpa.GenericNameDaoJpa">
    <constructor-arg value="org.glvnsjc.model.domain.Course" />
  </bean>

  <bean id="classDao" class="org.glvnsjc.internal.dao.jpa.GenericNameDaoJpa">
    <constructor-arg value="org.glvnsjc.model.domain.Clazz" />
  </bean>

  <bean id="termDao" class="org.glvnsjc.internal.dao.jpa.GenericNameDaoJpa">
    <constructor-arg value="org.glvnsjc.model.domain.Term" />
  </bean>

</beans>
